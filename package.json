{
  "name": "eslint-config-cybanjarjs",
  "version": "1.3.2",
  "description": "Eslint config for Javascript",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint -c ./index.js ./*.js",
    "fix": "eslint -c ./index.js ./*.js --fix",
    "test:lint": "bash ./script/lint.sh",
    "test:fix": "bash ./script/fix.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cybanjar/eslint-config-cybanjarjs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": {
    "name": "Syamsul Amin",
    "email": "cybanjar@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cybanjar/eslint-config-cybanjarjs/issues"
  },
  "homepage": "https://github.com/cybanjar/eslint-config-cybanjarjs#readme",
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-varspacing": "^1.2.2",
    "eslint-plugin-vue": "^9.17.0"
  }
}
